{
    "entitywriteindex": {
        "settings": {
            "analysis": {
                "analyzer": {
                    "path_hierarchy-analyzer": {
                        "type": "custom",
                        "tokenizer": "path_token"
                    }
                },
                "tokenizer": {
                    "path_token": {
                        "type": "path_hierarchy",
                        "delimiter": "/"
                    }
                }
            }
        },
        "mappings": {
            "entity": {
                "dynamic_templates": [
                    {
                        "nested_value_feature": {
                            "path_match": "data.ctxInfo.*values",
                            "mapping": {
                                "type": "nested"
                            }
                        }
                    },
                    {
                        "nested_row_feature": {
                            "path_match": "data.ctxInfo.*group",
                            "mapping": {
                                "type": "nested"
                            }
                        }
                    },
                    {
                        "ctx_path": {
                            "path_match": "data.ctxInfo.*ctxPath",
                            "mapping": {
                                "type": "text",
                                "analyzer": "path_hierarchy-analyzer",
                                "search_analyzer": "keyword"
                            }
                        }
                    },
                    {
                        "nested_attribute_feature": {
                            "path_match": "data.ctxInfo.*attributes",
                            "mapping": {
                                "type": "nested"
                            }
                        }
                    },
                    {
                        "nested_relationship_feature": {
                            "path_match": "data.ctxInfo.*relationships",
                            "mapping": {
                                "type": "nested"
                            }
                        }
                    },
                    {
                        "attrValue": {
                            "path_match": "data.ctxInfo.*attributes*.value",
                            "mapping": {
                                "type": "text",
                                "ignore_malformed": true,
                                "fielddata": true
                            }
                        }
                    },
                    {
                        "attrDateValue": {
                            "path_match": "data.ctxInfo.*attributes*.dateValue",
                            "mapping": {
                                "type": "date",
                                "format": "yyyy-MM-dd"
                            }
                        }
                    },
                    {
                        "attrDateTimeValue": {
                            "path_match": "data.ctxInfo.*attributes*.dateTimeValue",
                            "mapping": {
                                "type": "basic_date_time"
                            }
                        }
                    },
                    {
                        "attrDecimalValue": {
                            "path_match": "data.ctxInfo.*attributes*.decValue",
                            "mapping": {
                                "type": "float"
                            }
                        }
                    },
                    {
                        "attrIntegerValue": {
                            "path_match": "data.ctxInfo.*attributes*.intValue",
                            "mapping": {
                                "type": "long"
                            }
                        }
                    },
                    {
                        "attrTimestampValue": {
                            "path_match": "data.ctxInfo.*attributes*.timestamp",
                            "mapping": {
                                "type": "date",
                                "format": "epoch_millis"
                            }
                        }
                    },
                    {
                        "attrBooleanValue": {
                            "path_match": "data.ctxInfo.*attributes*.boolValue",
                            "mapping": {
                                "type": "boolean"
                            }
                        }
                    },
                    {
                        "attrInvalidValue": {
                            "path_match": "data.ctxInfo.*attributes*.invalidValue",
                            "mapping": {
                                "type": "text"
                            }
                        }
                    },
                    {
                        "propValue": {
                            "path_match": "data.ctxInfo.*propertyValue",
                            "mapping": {
                                "type": "text",
                                "ignore_malformed": true
                            }
                        }
                    },
                    {
                        "nestedProperty": {
                            "path_match": "data.ctxInfo.*properties",
                            "mapping": {
                                "type": "nested"
                            }
                        }
                    }
                ],
                "properties": {
                    "data.ctxInfo": {
                        "type": "nested"
                    },
                    "id": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            }
        }
    },
