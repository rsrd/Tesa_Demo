{
  "id": "4d05ecf6-1a9b-4d66-a89e-d2474a023c3c_jcpSKU_v2",
  "name": "JCPenney_Sku_Excel_Import_Transform",
  "profileType": "contentOnboarding",
  "dataInfo": "Entity",
  "enabled": "true",
  "collect": {
    "persistData": "true",
    "batchSize": 1,
    "folderChannel": {
      "sourceFolder": "/apps/import/sku",
      "pattern": "*.xlsx"
    },
    "format": {
      "type": "EXCEL",
      "version": "1.1",
      "settings": {
        "encoding": "utf8"
      }
    },
    "includeFilter": [],
    "excludeFilter": []
  },
  "publish": {
    "persistData": "true",
    "batchSize": 10000,
    "rdpChannel": {
      "format": {
        "type": "RSJSON",
        "version": "1.1"
      }
    },
    "includeFilter": [],
    "excludeFilter": []
  },
  "transform": {
    "fieldMapGenerator": {
      "mappingSettings": {
        "isFirstRowHeader": "true",
        "strategy": {
          "camelCase": "true",
          "removeSpecialCharacter": "true",
          "useHeaderAsIs": "true",
          "dataModelProperty": "shortName"
        }
      },
      "contexts": [
        {
          "context": [
            {
              "name": "classification",
              "fields": [
                "Sheet1.AA",
                "Sheet1.AB"
              ]
            }
          ]
        },
        {
          "context": [
            {
              "name": "channel",
              "identificationStrategy": "StaticValue",
              "fields": [
                "@value(Web)"
              ]
            },
            {
              "name": "classification",
              "fields": [
                "Sheet1.AC"
              ]
            }
          ]
        },
        {
          "name": "channel",
          "fields": [
            "Sheet1.AA",
            "Sheet1.AB"
          ]
        }
      ],
      "transformSettings": {
        "collectionSeparator": "||",
        "allowNullValues": "false",
        "mergeType": "clearMissingAttributes"
      }
    },
    "overrides": [
      {
        "source": "@excelColumn(Sheet1.A)",
        "target": "id",
        "default": "default value",
        "type": "int/bool/nested/collection/Unknown",
        "isRequired": "false"
      },
      {
        "source": "PRD TYP",
        "target": "type",
        "default": "default value",
        "type": "int/bool/nested/collection/Unknown",
        "isRequired": "false"
      },
      {
        "source": "@value(rms)",
        "target": "source"
      },
      {
        "source": "@date(4/3/2017)",
        "target": "createdDate"
      },
      {
        "source": "@value(ghostUser)",
        "target": "createdBy"
      },
      {
        "source": "@date(now)",
        "target": "modifiedDate"
      },
      {
        "source": "@value(another ghostUser)",
        "target": "modifiedBy"
      },
      {
        "source": "Column XYZ",
        "target": "csapDescriptionOfNart",
        "context": "list:poductMaster"
      },
      {
        "source": "Column ABC",
        "target": "description",
        "context": "self"
      },
      {
        "source": "Column DEF",
        "target": "description",
        "context": "channel:Web"
      }
    ]
  }
}