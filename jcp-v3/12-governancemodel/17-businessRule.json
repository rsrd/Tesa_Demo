gWeight<=120,SetVariable[\"baseCost\",46.57],false] AND IIF[billingWeight>=121 AND billingWeight<=130,SetVariable[\"baseCost\",50.01],false] AND IIF[billingWeight>=131 AND billingWeight<=140,SetVariable[\"baseCost\",53.52],false] AND IIF[billingWeight>=141 AND billingWeight<=150,SetVariable[\"baseCost\",57.04],false] AND IIF[IsNullOrEmpty[maxSide],true,SetVariable[\"maxSide\", NumericMax[varLength,varWidth,varHeight]]] AND IIF[unitedInches<135 and varGrossWeight<150,SetAttributeValue[\"estimatedupsshippingcharge\",(baseCost+ 0.5+1.39)],false] AND IIF[maxSide>60 and varGrossWeight>150,SetAttributeValue[\"estimatedupsshippingcharge\",(baseCost+ 3.0+1.64)],false] AND IIF[unitedInches>135 and unitedInches<165,SetAttributeValue[\"estimatedupsshippingcharge\",(baseCost+ 68.0+10.38]),false] AND IIF[unitedInches>165,SetAttributeValue[\"estimatedupsshippingcharge\",150],false ]"
